{
  "expo": {
    "name": "WeCare",
    "slug": "wecare-fe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-icon-dark.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wecare.app",
      "icon": "./assets/splash-icon-light.png"
    },
    "android": {
      "package": "com.wecare.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon-light.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/splash-icon-dark.png",
        "color": "#FF231F7C",
        "adaptiveIcon": {
          "foregroundImage": "./assets/splash-icon-dark.png",
          "backgroundColor": "#FF231F7C"
        }
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "package": "com.wecare.app",
      "favicon": "./assets/Icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "45cd0beb-6343-41db-a414-7e2e6b34443e"
      }
    },
    "plugins": [
      "expo-font",
      "expo-asset"
    ]
  }
}
